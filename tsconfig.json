{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es7",
      "es6"
    ],
    "noImplicitAny": true,
    "outDir": "./lib",
    "types": ["node"]
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}